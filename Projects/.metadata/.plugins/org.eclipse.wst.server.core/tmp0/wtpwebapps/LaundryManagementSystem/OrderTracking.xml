<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
          http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
          version="3.1">
<!--Insert data part -->
    <servlet>
   		 <servlet-name>OrderTrackServlet</servlet-name>
   		 <servlet-class>Servlet.OrderTrackServlet</servlet-class>
    </servlet>
    
      <servlet-mapping>
        <servlet-name>OrderTrackServlet</servlet-name>
        <url-pattern>/OrderTrackServlet</url-pattern>
    </servlet-mapping>
    
    
    <!--Read data part -->
    <servlet>
   		 <servlet-name>OrderTrackReadServlet</servlet-name>
   		 <servlet-class>Servlet.OrderTrackReadServlet</servlet-class>
    </servlet>
    
      <servlet-mapping>
        <servlet-name>OrderTrackReadServlet</servlet-name>
        <url-pattern>/OrderTrackReadServlet</url-pattern>
    </servlet-mapping>
    
    
      <!--Update data part -->
    <servlet>
   		 <servlet-name>OrderTrackUpdateServlet</servlet-name>
   		 <servlet-class>Servlet.OrderTrackUpdateServlet</servlet-class>
    </servlet>
    
      <servlet-mapping>
        <servlet-name>OrderTrackUpdateServlet</servlet-name>
        <url-pattern>/OrderTrackUpdateServlet</url-pattern>
    </servlet-mapping>
    
    
    
     <!--Delete data part -->
   <servlet>
    	<servlet-name>OrderTrackDeleteServlet</servlet-name>
    	<servlet-class>Servlet.OrderTrackDeleteServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
    	<servlet-name>OrderTrackDeleteServlet</servlet-name>
    	<url-pattern>/OrderTrackDeleteServlet</url-pattern>
	</servlet-mapping>

 
    
</web-app>
