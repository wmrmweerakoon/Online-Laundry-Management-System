<web-app 
xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    
    <servlet>
        <servlet-name>addOrder</servlet-name>
        <servlet-class>servlet.addOrder</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>addOrder</servlet-name>
        <url-pattern>/addOrder</url-pattern>
    </servlet-mapping>
    
    
      <servlet>
        <servlet-name>UpdateOrder</servlet-name>
        <servlet-class>servlet.UpdateOrder</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>UpdateOrder</servlet-name>
        <url-pattern>/UpdateOrder</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DeleteOrder</servlet-name>
        <servlet-class>servlet.DeleteOrder</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>DeleteOrder</servlet-name>
        <url-pattern>/DeleteOrder</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>ShowData</servlet-name>
    <servlet-class>servlet.ShowData</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>ShowData</servlet-name>
    <url-pattern>/ShowData</url-pattern>
    </servlet-mapping>
    
    </web-app>